<style type="text/css">
    .middle {
        text-align: center;
    }
    body {
        padding-top: 60px;
        padding-bottom: 40px;
    }
    .sidebar-nav {
        padding: 9px 0;
    }
    #StatusBar
    {
        position: fixed;
        bottom: 0px;
        display: block;
        width: 100%;
        height: 15px;
        z-index: 100;
        margin:0;
        color:black;
    }
    #maintab { padding: 0px; }
    .contentHead {
        height:56px;
    }
    .contentBody {
    }
    .contentFoot {
        height:130px;
    }
    .hover-input {
        border:0px solid #ccc !important;
        background-color: transparent !important;
        box-shadow: none !important;
    }
    .hover-input:hover {
        border:1px solid #ccc !important;
        background-color: white !important;
    }
    #chat-wrap {
    }
    .activeline {background: #e8f2ff !important;}
   
</style>

</head>

<body>
    <div id="header">
    <div class="navbar navbar-fixed-top" id="header">
        <div class="navbar-inner">
            <div class="container-fluid">
                <div class="row-fluid">
                    <div style="float:right">
                      <button class="btn btn-large btn-danger" id="copybutton">Copy Link</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>
    <div id="wrapper"><div class="content">
    <div class="container-fluid" id="main" style="height: 172px;">
        <div class="row-fluid">
            <div class="span9">
                <div class="contentBody" style="height: 172px;">
                    <ul class="nav nav-tabs" id="maintab">
                        <l1><span id="lang">C++</span></li>
                        <li style="float:right" class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#"><i class="icon icon-cogs"></i>&nbsp;Select Language <b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="cpp"><i class="icon icon-adjust"></i>&nbsp;C++</a></li>
                            <li><a href="#" id="php"><i class="icon icon-adjust"></i>&nbsp;PHP</a></li>
                            <li><a href="#" id="ruby"><i class="icon icon-adjust"></i>&nbsp;Ruby</a></li>
                            <li><a href="#" id="python"><i class="icon icon-adjust"></i>&nbsp;Python</a></li>
                        </ul>
                        </li>
                    </ul>
                <div class="tab-pane" id="editor">
                        <textarea id="codeeditor" name="code"></textarea>
                </div><!--/contentBody-->
                </div><!--/contentFoot-->
            </div><!--/span-->
            <div class="span3 leftNav" style="height: 142px;">
                <div class="well sidebar-nav" id="participants">
                   
                    <ul class="nav nav-list">
                    </ul>
                </div><!--/.well -->
                <div class="thumbnail" id="chatwindow">
                    <div id="chat" style="padding: 0px 5px; width: 303px; height: 42px; overflow-y: auto; word-wrap: break-word;">
                     </div>
                    <div id="chatbox" style="margin-top:10px;">
                        <textarea style="height:3.8em;width:97%;" id="text-input"></textarea>
                    </div>
                </div><!--/.well -->
            </div><!--/span-->
        </div><!--/row-->
    </div><!--/.fluid-container-->
   
    </div><div class="footer">
    <div id="StatusBar" class="alert"></div>
    </div></div>
        <script type="text/javascript" src="<%= cdn_path %>channel/bcsocket.js"></script>
        <script type="text/javascript" src="<%= cdn_path %>share/share.js"></script>
